{
  "name": "@keg-hub/jsutils",
  "version": "9.7.8",
  "description": "Keg common javascript utils",
  "main": "build/cjs/index.js",
  "module": "build/esm/index.js",
  "browser": "build/esm/index.js",
  "types": "build/esm/index.d.ts",
  "exports": {
    "./node": "./build/cjs/node/node.js",
    "./src/node": "./build/cjs/node/node.js",
    "./src/node/*": "./build/cjs/node/node.js",
    "./node/*": {
      "import": "./build/esm/node/node.js",
      "require": "./build/cjs/node/node.js",
      "default": "./build/cjs/node/node.js"
    },
    "./*": {
      "import": "./build/esm/*.js",
      "require": "./build/cjs/*.js",
      "default": "./build/cjs/*.js"
    },
    ".": {
      "import": "./build/esm/index.js",
      "require": "./build/cjs/index.js",
      "default": "./build/cjs/index.js"
    }
  },
  "author": "lancetipton <lancetipton04@gmail.com>",
  "license": "MIT",
  "repository": {
    "url": "git+https://github.com/keg-hub/keg-hub.git",
    "type": "git"
  },
  "bugs": {
    "url": "https://github.com/keg-hub/keg-hub/issues"
  },
  "homepage": "https://github.com/keg-hub/keg-hub#readme",
  "files": [
    "build"
  ],
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "keg",
    "keg-hub",
    "utils",
    "helpers"
  ],
  "scripts": {
    "prebuild": "pnpm format && pnpm test",
    "build": "pnpm build:es && pnpm types",
    "build:es": "node --enable-source-maps -r esbuild-register configs/build.config.ts",
    "build:clean": "rm -rf ./build",
    "build:test": "node -r esbuild-register ./scripts/testBuild.ts",
    "clean:nm": "rm -rf node_modules",
    "clean:full": "pnpm clean && pnpm install",
    "clean": "pnpm build:clean && pnpm clean:nm",
    "build:docs": "node -r esbuild-register ./scripts/buildDocs.js",
    "entries": "node -r esbuild-register ./scripts/getEntries.ts",
    "format:eslint": "eslint --config ./configs/eslintrc.config.js . --fix --quiet",
    "format:prettier": "prettier --config ./configs/prettier.config.js --ignore-path .eslintignore --write '**/*.ts'",
    "format": "pnpm format:prettier && pnpm format:eslint",
    "test": "NODE_ENV=test jest --verbose --config=./configs/jest.config.js  --maxWorkers=50%",
    "prettier": "prettier",
    "types": "tsc && pnpm types:fix",
    "types:fix": "node --enable-source-maps -r esbuild-register scripts/fixTypes.ts",
    "validate": "tsc --noEmit",
    "web": "npx http-server ./docs -p 8888 --cors"
  },
  "devDependencies": {
    "@jgoz/esbuild-plugin-typecheck": "3.0.2",
    "@types/debug": "4.1.5",
    "@types/jest": "29.5.2",
    "@types/node": "18.15.13",
    "@typescript-eslint/parser": "5.60.1",
    "docdash": "1.2.0",
    "esbuild": "0.18.17",
    "esbuild-jest": "0.5.0",
    "esbuild-plugin-d.ts": "1.1.0",
    "esbuild-plugin-path-alias": "1.0.7",
    "esbuild-register": "3.4.2",
    "eslint": "8.44.0",
    "eslint-plugin-jest": "27.0.1",
    "gh-pages": "5.0.0",
    "glob": "10.3.3",
    "jest": "29.5.0",
    "jsdoc": "3.6.11",
    "jsdom": "^20.0.0",
    "jsutils": "file:.",
    "prettier": "2.7.1",
    "typescript": "5.1.6"
  },
  "directories": {
    "doc": "docs"
  },
  "publish": {
    "repository": "https://github.com/keg-hub/jsutils.git"
  }
}
