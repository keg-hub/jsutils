{
  "tags": {
    "allowUnknownTags": true
  },
  "source": {
    "include": [
      "src",
      "README.md"
    ],
    "excludePattern": "(__mocks__|__tests__|node_modules/|docs)"
  },
  "plugins": [
    "plugins/markdown",
    "plugins/summarize"
  ],
  "opts": {
    "template": "node_modules/tsd-jsdoc/dist",
    "encoding": "utf8",
    "destination": ".",
    "recurse": true,
    "verbose": true
  }
}